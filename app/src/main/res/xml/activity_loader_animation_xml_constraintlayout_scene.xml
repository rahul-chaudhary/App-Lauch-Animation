                motion:keyPositionType="deltaRelative"
                motion:percentX="1" />
            <KeyPosition
                motion:keyPositionType="deltaRelative"
                motion:percentX=".50"
                motion:motionTarget="@+id/movingIV"
                motion:framePosition="60" />
            <KeyPosition
                motion:keyPositionType="deltaRelative"
                motion:percentX=".50"
                motion:motionTarget="@+id/movingIV"
                motion:framePosition="40" />
            <KeyAttribute
                motion:motionTarget="@+id/firstIV"
                motion:framePosition="40"
                android:scaleX="0.9"
                android:scaleY="0.9" />
            <KeyAttribute
                motion:motionTarget="@+id/firstIV"
                motion:framePosition="20"
                android:scaleX="1"
                android:scaleY="1" />
            <KeyAttribute
                android:scaleX="0.9"
                android:scaleY="0.9"
                motion:motionTarget="@+id/firstIV"
                motion:framePosition="100" />
            <KeyAttribute
                motion:motionTarget="@+id/secondIV"
                motion:framePosition="40"
                android:scaleX="1"
                android:scaleY="1" />
            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                motion:motionTarget="@+id/secondIV"
                motion:framePosition="60" />
            <KeyAttribute
                android:scaleX="0.9"
                android:scaleY="0.9"
                motion:motionTarget="@+id/secondIV"
                motion:framePosition="80" />
            <KeyAttribute
                android:scaleX="0.9"
                android:scaleY="0.9"
                motion:motionTarget="@+id/secondIV"
                motion:framePosition="100" />
            <KeyAttribute
                android:scaleX="0.9"
                android:scaleY="0.9"
                motion:motionTarget="@+id/secondIV"
                motion:framePosition="20" />
            <KeyAttribute
                motion:motionTarget="@+id/thirdIV"
                motion:framePosition="60"
                android:scaleX="0.9"
                android:scaleY="0.9" />
            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                motion:motionTarget="@+id/thirdIV"
                motion:framePosition="80" />
            <KeyAttribute
                android:scaleX="1"
                android:scaleY="1"
                motion:motionTarget="@+id/thirdIV"
                motion:framePosition="100" />
        </KeyFrameSet>
    </Transition>

