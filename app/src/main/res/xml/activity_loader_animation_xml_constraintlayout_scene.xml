<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:autoTransition="animateToEnd"
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="2000"
        motion:motionInterpolator="linear">
        <KeyFrameSet>
            <KeyAttribute
                android:scaleX="1"
                motion:framePosition="50"
                motion:motionTarget="@+id/secondIV" />
            <KeyAttribute
                android:scaleY="1"
                motion:framePosition="50"
                motion:motionTarget="@+id/secondIV" />

            <KeyAttribute
                android:alpha=".5"
                motion:framePosition="0"
                motion:motionTarget="@+id/eatTV" />

            <KeyAttribute
                android:alpha=".5"
                motion:framePosition="0"
                motion:motionTarget="@+id/saveTV" />

            <KeyAttribute
                android:alpha=".5"
                motion:framePosition="50"
                motion:motionTarget="@+id/bookTV" />

            <KeyAttribute
                android:alpha=".5"
                motion:framePosition="50"
                motion:motionTarget="@+id/saveTV" />

            <KeyAttribute
                android:alpha="1"
                motion:framePosition="50"
                motion:motionTarget="@+id/eatTV" />

            <KeyAttribute
                android:alpha=".5"
                motion:framePosition="100"
                motion:motionTarget="@+id/bookTV" />

            <KeyAttribute
                android:alpha=".5"
                motion:framePosition="100"
                motion:motionTarget="@+id/eatTV" />
            <KeyPosition
                motion:motionTarget="@+id/movingIV"
                motion:framePosition="30"
                motion:keyPositionType="deltaRelative"
                motion:percentX="0" />
            <KeyPosition
                motion:motionTarget="@+id/movingIV"
                motion:framePosition="66"
                motion:keyPositionType="deltaRelative"
                motion:percentX="1.0" />
            <KeyPosition
                motion:motionTarget="@+id/movingIV"
                motion:framePosition="33"
                motion:keyPositionType="deltaRelative"
                motion:percentX=".56" />
            <KeyPosition
                motion:motionTarget="@+id/movingIV"
                motion:framePosition="62"
                motion:keyPositionType="deltaRelative"
                motion:percentX=".56" />
            <KeyAttribute
                motion:motionTarget="@+id/secondIV"
                motion:framePosition="30"
                android:scaleX="0.5" />
            <KeyAttribute
                motion:motionTarget="@+id/secondIV"
                motion:framePosition="30"
                android:scaleY="0.5" />
            <KeyAttribute
                motion:motionTarget="@+id/thirdIV"
                motion:framePosition="62"
                android:scaleX="0.5" />
            <KeyAttribute
                motion:motionTarget="@+id/thirdIV"
                motion:framePosition="62"
                android:scaleY="0.5" />
            <KeyAttribute
                motion:motionTarget="@+id/thirdIV"
                motion:framePosition="90"
                android:scaleX="1"
                android:scaleY="1" />
            <KeyAttribute
                motion:motionTarget="@+id/thirdIV"
                android:scaleY="1.2" />

        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/secondIV"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/dp_15"
            android:elevation="@dimen/toolbar_elevation"
            android:scaleX="0.5"
            android:scaleY="0.5"
            motion:layout_constraintBottom_toBottomOf="@id/centerGuide"
            motion:layout_constraintDimensionRatio="1:1"
            motion:layout_constraintEnd_toStartOf="@+id/thirdIV"
            motion:layout_constraintStart_toEndOf="@+id/firstIV"
            motion:layout_constraintTop_toTopOf="@id/centerGuide"
            motion:layout_constraintWidth_percent=".14" />
        <Constraint
            android:id="@+id/thirdIV"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="-10dp"
            android:layout_marginEnd="@dimen/dp_20"
            android:elevation="@dimen/toolbar_elevation"
            android:scaleX="0.5"
            android:scaleY="0.5"
            motion:layout_constraintBottom_toBottomOf="@id/centerGuide"
            motion:layout_constraintDimensionRatio="1:1"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toEndOf="@+id/secondIV"
            motion:layout_constraintTop_toTopOf="@id/centerGuide"
            motion:layout_constraintWidth_percent=".14" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/movingIV"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:scaleX="1.3"
            android:scaleY="1.3"
            motion:layout_constraintDimensionRatio="1:1"

            motion:layout_constraintStart_toStartOf="@id/thirdIV"
            motion:layout_constraintTop_toTopOf="@id/thirdIV"
            motion:layout_constraintWidth_percent="0.18" />
        <Constraint
            android:id="@+id/secondIV"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="@dimen/dp_15"
            android:elevation="@dimen/toolbar_elevation"
            android:scaleX="0.5"
            android:scaleY="0.5"
            motion:layout_constraintBottom_toBottomOf="@id/centerGuide"
            motion:layout_constraintDimensionRatio="1:1"
            motion:layout_constraintEnd_toStartOf="@+id/thirdIV"
            motion:layout_constraintStart_toEndOf="@+id/firstIV"
            motion:layout_constraintTop_toTopOf="@id/centerGuide"
            motion:layout_constraintWidth_percent=".14" />
        <Constraint
            android:id="@+id/firstIV"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:elevation="@dimen/toolbar_elevation"
            android:scaleX="0.5"
            android:scaleY="0.5"
            motion:layout_constraintBottom_toBottomOf="@+id/centerGuide"
            motion:layout_constraintDimensionRatio="1:1"
            motion:layout_constraintEnd_toStartOf="@+id/secondIV"
            motion:layout_constraintHorizontal_chainStyle="packed"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="@id/centerGuide"
            motion:layout_constraintWidth_percent=".14" />
    </ConstraintSet>
</MotionScene>